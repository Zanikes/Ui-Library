        end

        function Page:CreateToggle(Text, Callback)
            local Callback = Callback or function() end

            local Toggle = Instance.new("TextLabel")
            local ToggleRound = Instance.new("ImageLabel")
            local ToggleText = Instance.new("TextLabel")
            local ToggleBackground = Instance.new("ImageLabel")
            local ToggleButton = Instance.new("TextButton")
            local ToggleButtonRound = Instance.new("ImageLabel")

            Toggle.Name = "Toggle"
            Toggle.Parent = Category
            Toggle.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
            Toggle.BackgroundTransparency = 1.000
            Toggle.BorderSizePixel = 0
            Toggle.Position = UDim2.new(0, 5, 0, 30)
            Toggle.Size = UDim2.new(0, 340, 0, 20)
            Toggle.ZIndex = 3
            Toggle.Font = Enum.Font.SourceSans
            Toggle.Text = ""
            Toggle.TextColor3 = Color3.fromRGB(255, 255, 255)
            Toggle.TextScaled = true
            Toggle.TextSize = 14.000
            Toggle.TextWrapped = true
            Toggle.LayoutOrder = LayoutOrder
            LayoutOrder = LayoutOrder + 1
            
            ToggleRound.Name = "ToggleRound"
            ToggleRound.Parent = Toggle
            ToggleRound.AnchorPoint = Vector2.new(0.5, 0.5)
            ToggleRound.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ToggleRound.BackgroundTransparency = 1.000
            ToggleRound.Position = UDim2.new(0.5, 0, 0.5, 0)
            ToggleRound.Size = UDim2.new(1, 0, 1, 0)
            ToggleRound.ZIndex = 2
            ToggleRound.Image = "rbxassetid://3570695787"
            ToggleRound.ImageColor3 = Color3.fromRGB(30, 30, 30)
            ToggleRound.ScaleType = Enum.ScaleType.Slice
            ToggleRound.SliceCenter = Rect.new(100, 100, 100, 100)
            ToggleRound.SliceScale = 0.040
            
            ToggleText.Name = "ToggleText"
            ToggleText.Parent = Toggle
            ToggleText.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ToggleText.BackgroundTransparency = 1.000
            ToggleText.Position = UDim2.new(0, 5, 0, 0)
            ToggleText.Size = UDim2.new(0, 150, 1, 0)
            ToggleText.ZIndex = 2
            ToggleText.Font = Enum.Font.SourceSans
            ToggleText.Text = Text or "New Toggle"
            ToggleText.TextColor3 = Color3.fromRGB(255, 255, 255)
            ToggleText.TextScaled = true
            ToggleText.TextSize = 14.000
            ToggleText.TextWrapped = true
            
            ToggleBackground.Name = "ToggleBackground"
            ToggleBackground.Parent = Toggle
            ToggleBackground.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ToggleBackground.BackgroundTransparency = 1.000
            ToggleBackground.Position = UDim2.new(0, 240, 0, 0)
            ToggleBackground.Size = UDim2.new(0, 100, 0, 25)
            ToggleBackground.ZIndex = 2
            ToggleBackground.Image = "rbxassetid://3570695787"
            ToggleBackground.ImageColor3 = Color3.fromRGB(50, 50, 50)
            ToggleBackground.ScaleType = Enum.ScaleType.Slice
            ToggleBackground.SliceCenter = Rect.new(100, 100, 100, 100)
            ToggleBackground.SliceScale = 0.040
            
            ToggleButton.Name = "ToggleButton"
            ToggleButton.Parent = ToggleBackground
            ToggleButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
            ToggleButton.BackgroundTransparency = 1.000
            ToggleButton.BorderSizePixel = 0
            ToggleButton.ClipsDescendants = true
            ToggleButton.Position = UDim2.new(0, 5, 0, 3)
            ToggleButton.Size = UDim2.new(0.5, 0, 1, -6)
            ToggleButton.ZIndex = 3
            ToggleButton.Font = Enum.Font.SourceSans
            ToggleButton.Text = "X"
            ToggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
            ToggleButton.TextSize = 14.000
            ToggleButton.TextWrapped = true
            ToggleButton.TextScaled = true

            local ms = game.Players.LocalPlayer:GetMouse()
            local Tog = false

            ToggleButton.MouseButton1Click:Connect(function()
                if Tog == false then
                    Tog = true
                    ToggleButton.Text = utf8.char(10003)
                    pcall(Callback, Tog)
                    game:GetService("TweenService"):Create(ToggleButtonRound, TweenInfo.new(0.3), {ImageColor3 = Color3.fromRGB(0,255,0)}):Play()
                    ToggleButton:TweenPosition(UDim2.new(1,-55,0,3), 'In', 'Linear', 0.1)
                else
                    Tog = false
                    ToggleButton.Text = "X"
                    pcall(Callback, Tog)
                    game:GetService("TweenService"):Create(ToggleButtonRound, TweenInfo.new(0.3), {ImageColor3 = Color3.fromRGB(255,0,0)}):Play()
                    ToggleButton:TweenPosition(UDim2.new(0,5,0,3), 'In', 'Linear', 0.1)
                end
                
                local c = Sample2:Clone()
                c.Parent = ToggleButton
                local x, y = (ms.X - c.AbsolutePosition.X), (ms.Y - c.AbsolutePosition.Y)
                c.Position = UDim2.new(0, x, 0, y)
                local len, size = 1, nil
                if ToggleButton.AbsoluteSize.X >= ToggleButton.AbsoluteSize.Y then
                    size = (ToggleButton.AbsoluteSize.X * 1.5)
                else
                    size = (ToggleButton.AbsoluteSize.Y * 1.5)
                end
                c:TweenSizeAndPosition(UDim2.new(0, size, 0, size), UDim2.new(0.5, (-size / 2), 0.5, (-size / 2)), 'Out', 'Quad', len, true, nil)
                for i = 1, 10 do
                    c.ImageTransparency = c.ImageTransparency + 0.05
                    wait(len / 12)
                end
                c:Destroy()
            end)
            
            ToggleButtonRound.Name = "ToggleButtonRound"
            ToggleButtonRound.Parent = ToggleButton
            ToggleButtonRound.Active = true
            ToggleButtonRound.AnchorPoint = Vector2.new(0.5, 0.5)
            ToggleButtonRound.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            ToggleButtonRound.BackgroundTransparency = 1.000
            ToggleButtonRound.Position = UDim2.new(0.5, 0, 0.5, 0)
            ToggleButtonRound.Selectable = true
            ToggleButtonRound.Size = UDim2.new(1, 0, 1, 0)
            ToggleButtonRound.ZIndex = 2
            ToggleButtonRound.Image = "rbxassetid://3570695787"
            ToggleButtonRound.ImageColor3 = Color3.fromRGB(255, 0, 0)
            ToggleButtonRound.ScaleType = Enum.ScaleType.Slice
            ToggleButtonRound.SliceCenter = Rect.new(100, 100, 100, 100)
            ToggleButtonRound.SliceScale = 0.040
        end

        function Page:CreateSlider(Text, Min, Max, Callback)
            minvalue = Min or 0
            maxvalue = Max or 100
            local callback = Callback or function() end

            local mouse = game.Players.LocalPlayer:GetMouse()
            local uis = game:GetService("UserInputService")
            local Value

            local Slider = Instance.new("ImageLabel")
            local SliderName = Instance.new("TextLabel")
            local SliderHolder = Instance.new("TextButton")
            local SliderRound = Instance.new("ImageLabel")
            local SliderMovingPart = Instance.new("ImageLabel")
            local SliderValue = Instance.new("TextLabel")

            Slider.Name = "Slider"
            Slider.Parent = Category
            Slider.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Slider.BackgroundTransparency = 1.000
            Slider.Size = UDim2.new(0, 100, 0, 100)
            Slider.ZIndex = 2
            Slider.Image = "rbxassetid://3570695787"
            Slider.ImageColor3 = Color3.fromRGB(30, 30, 30)
            Slider.ScaleType = Enum.ScaleType.Slice
            Slider.SliceCenter = Rect.new(100, 100, 100, 100)
            Slider.SliceScale = 0.040
            Slider.LayoutOrder = LayoutOrder
            LayoutOrder = LayoutOrder + 1

            SliderName.Name = "SliderName"
            SliderName.Parent = Slider
            SliderName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            SliderName.BackgroundTransparency = 1.000
            SliderName.Size = UDim2.new(0, 125, 1, 0)
            SliderName.ZIndex = 2
            SliderName.Font = Enum.Font.SourceSans
            SliderName.Text = Text or "New Slider"
            SliderName.TextColor3 = Color3.fromRGB(255, 255, 255)
            SliderName.TextScaled = true
            SliderName.TextSize = 14.000
            SliderName.TextWrapped = true

            SliderHolder.Name = "SliderHolder"
            SliderHolder.Parent = Slider
            SliderHolder.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            SliderHolder.BackgroundTransparency = 1.000
            SliderHolder.BorderSizePixel = 0
            SliderHolder.Position = UDim2.new(0, 125, 0, 0)
            SliderHolder.Size = UDim2.new(0, 215, 0, 25)
            SliderHolder.ZIndex = 3
            SliderHolder.Font = Enum.Font.SourceSans
            SliderHolder.Text = ""
            SliderHolder.TextColor3 = Color3.fromRGB(0, 0, 0)
            SliderHolder.TextSize = 14.000

            SliderRound.Name = "SliderRound"
            SliderRound.Parent = SliderHolder
            SliderRound.Active = true
            SliderRound.AnchorPoint = Vector2.new(0.5, 0.5)
            SliderRound.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            SliderRound.BackgroundTransparency = 1.000
            SliderRound.Position = UDim2.new(0.5, 0, 0.5, 0)
            SliderRound.Selectable = true
            SliderRound.Size = UDim2.new(1, 0, 1, 0)
            SliderRound.ZIndex = 2
            SliderRound.Image = "rbxassetid://3570695787"
            SliderRound.ImageColor3 = Color3.fromRGB(50, 50, 50)
            SliderRound.ScaleType = Enum.ScaleType.Slice
            SliderRound.SliceCenter = Rect.new(100, 100, 100, 100)
            SliderRound.SliceScale = 0.040

            SliderMovingPart.Name = "SliderMovingPart"
            SliderMovingPart.Parent = SliderHolder
            SliderMovingPart.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            SliderMovingPart.BackgroundTransparency = 1.000
            SliderMovingPart.Size = UDim2.new(0, 0, 1, 0)
            SliderMovingPart.ZIndex = 2
            SliderMovingPart.Image = "rbxassetid://3570695787"
            SliderMovingPart.ImageColor3 = Color3.fromRGB(77, 166, 255)
            SliderMovingPart.ScaleType = Enum.ScaleType.Slice
            SliderMovingPart.SliceCenter = Rect.new(100, 100, 100, 100)
            SliderMovingPart.SliceScale = 0.040

            SliderValue.Name = "SliderValue"
            SliderValue.Parent = SliderHolder
            SliderValue.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            SliderValue.BackgroundTransparency = 1.000
            SliderValue.Size = UDim2.new(1, 0, 1, 0)
            SliderValue.ZIndex = 2
            SliderValue.Font = Enum.Font.SourceSans
            SliderValue.Text = "0"
            SliderValue.TextColor3 = Color3.fromRGB(255, 255, 255)
            SliderValue.TextScaled = true
            SliderValue.TextSize = 14.000
            SliderValue.TextWrapped = true

            SliderHolder.MouseButton1Down:Connect(function()
                Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 215) * SliderMovingPart.AbsoluteSize.X) + tonumber(minvalue)) or 0
                SliderValue.Text = Value
                pcall(function()
                    callback(Value)
                end)
                SliderMovingPart:TweenSize(UDim2.new(0, math.clamp(mouse.X - SliderMovingPart.AbsolutePosition.X, 0, 215), 0, 25), 'InOut', 'Linear', 0.1)
                moveconnection = mouse.Move:Connect(function()
                    SliderValue.Text = Value
                    Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 215) * SliderMovingPart.AbsoluteSize.X) + tonumber(minvalue))
                    pcall(function()
                        callback(Value)
                    end)
                    SliderMovingPart:TweenSize(UDim2.new(0, math.clamp(mouse.X - SliderMovingPart.AbsolutePosition.X, 0, 215), 0, 25), 'InOut', 'Linear', 0.1)
                end)
                releaseconnection = uis.InputEnded:Connect(function(Mouse)
                    if Mouse.UserInputType == Enum.UserInputType.MouseButton1 then
                        Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 215) * SliderMovingPart.AbsoluteSize.X) + tonumber(minvalue))
                        SliderValue.Text = Value
                        pcall(function()
                            callback(Value)
                        end)
                        SliderMovingPart:TweenSize(UDim2.new(0, math.clamp(mouse.X - SliderMovingPart.AbsolutePosition.X, 0, 215), 0, 25), 'InOut', 'Linear', 0.1)
                        moveconnection:Disconnect()
                        releaseconnection:Disconnect()
                    end
                end)
            end)
        end

        function Page:CreateDropdown(Text, Choices, Callback)
            local Callback = Callback or function() end

            local Dropdown = Instance.new("ImageLabel")
            local DropdownName = Instance.new("TextLabel")
            local DropdownButton = Instance.new("TextButton")
            local DropdownRound = Instance.new("ImageLabel")
            local Icon = Instance.new("TextLabel")

            local DropdownFrameTop = Instance.new("TextLabel")
            local DropdownTopRound = Instance.new("ImageLabel")
            local DropdownFrame = Instance.new("ImageLabel")
            local DropdownButtons = Instance.new("ScrollingFrame")
            local UIGridLayoutDropdown = Instance.new("UIGridLayout")
            local DropdownFiller = Instance.new("Frame")

            DropdownFrameTop.Name = "DropdownFrameTop"
            DropdownFrameTop.Parent = Mainframe
            DropdownFrameTop.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            DropdownFrameTop.BackgroundTransparency = 1.000
            DropdownFrameTop.BorderSizePixel = 0
            DropdownFrameTop.Position = UDim2.new(0, 455, 0, 0)
            DropdownFrameTop.Size = UDim2.new(0, 0, 0, 40)
            DropdownFrameTop.ZIndex = 3
            DropdownFrameTop.Font = Enum.Font.SourceSans
            DropdownFrameTop.Text = "Dropdown - Name"
            DropdownFrameTop.TextColor3 = Color3.fromRGB(255, 255, 255)
            DropdownFrameTop.TextScaled = true
            DropdownFrameTop.TextSize = 14.000
            DropdownFrameTop.TextWrapped = true
            DropdownFrameTop.Visible = false

            DropdownTopRound.Name = "DropdownTopRound"
            DropdownTopRound.Parent = DropdownFrameTop
            DropdownTopRound.AnchorPoint = Vector2.new(0.5, 0.5)
            DropdownTopRound.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            DropdownTopRound.BackgroundTransparency = 1.000
            DropdownTopRound.Position = UDim2.new(0.5, 0, 0.5, 0)
            DropdownTopRound.Size = UDim2.new(1, 0, 1, 0)
            DropdownTopRound.ZIndex = 2
            DropdownTopRound.Image = "rbxassetid://3570695787"
            DropdownTopRound.ImageColor3 = Color3.fromRGB(50, 50, 50)
            DropdownTopRound.ScaleType = Enum.ScaleType.Slice
            DropdownTopRound.SliceCenter = Rect.new(100, 100, 100, 100)
            DropdownTopRound.SliceScale = 0.080

            DropdownFrame.Name = "DropdownFrame"
            DropdownFrame.Parent = DropdownFrameTop
            DropdownFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            DropdownFrame.BackgroundTransparency = 1.000
            DropdownFrame.ClipsDescendants = true
            DropdownFrame.Size = UDim2.new(0, 195, 0, 0)
            DropdownFrame.Image = "rbxassetid://3570695787"
            DropdownFrame.ImageColor3 = Color3.fromRGB(40, 40, 40)
            DropdownFrame.ScaleType = Enum.ScaleType.Slice
            DropdownFrame.SliceCenter = Rect.new(100, 100, 100, 100)
            DropdownFrame.SliceScale = 0.080

            DropdownButtons.Name = "DropdownButtons"
            DropdownButtons.Parent = DropdownFrame
            DropdownButtons.Active = true
            DropdownButtons.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            DropdownButtons.BackgroundTransparency = 1.000
            DropdownButtons.BorderSizePixel = 0
            DropdownButtons.Position = UDim2.new(0, 0, 0, 40)
            DropdownButtons.Size = UDim2.new(1, 0, 1, -40)
            DropdownButtons.BottomImage = ""
            DropdownButtons.CanvasSize = UDim2.new(0, 0, 0, 0)
            DropdownButtons.ScrollBarThickness = 1
            DropdownButtons.TopImage = ""

            for i = 1, #Choices do
                DropdownButtons.CanvasSize = DropdownButtons.CanvasSize + UDim2.new(0, 0, 0, 25)

                local Choice = Instance.new("TextButton")

                Choice.Name = "Choice"
                Choice.Parent = DropdownButtons
                Choice.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
                Choice.BorderColor3 = Color3.fromRGB(30, 30, 30)
                Choice.BorderSizePixel = 0
                Choice.Size = UDim2.new(0, 200, 0, 50)
                Choice.AutoButtonColor = false
                Choice.Font = Enum.Font.SourceSans
                Choice.Text = Choices[i]
                Choice.TextColor3 = Color3.fromRGB(255, 255, 255)
                Choice.TextScaled = true
                Choice.TextSize = 14.000
                Choice.TextWrapped = true
                Choice.ClipsDescendants = true

                Choice.MouseEnter:Connect(function()
                    game:GetService("TweenService"):Create(Choice, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(50,50,50)}):Play()
                end)
        
                Choice.MouseLeave:Connect(function()
                    game:GetService("TweenService"):Create(Choice, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(40,40,40)}):Play()
                end)

                Choice.MouseButton1Click:Connect(function()
                    DropdownButton.Text = " " .. Choice.Text
                    pcall(Callback, Choice.Text)

                    local c = Sample2:Clone()
                    c.Parent = Choice
                    local x, y = (ms.X - c.AbsolutePosition.X), (ms.Y - c.AbsolutePosition.Y)
                    c.Position = UDim2.new(0, x, 0, y)
                    local len, size = 1, nil
                    if Choice.AbsoluteSize.X >= Choice.AbsoluteSize.Y then
                        size = (Choice.AbsoluteSize.X * 1.5)
                    else
                        size = (Choice.AbsoluteSize.Y * 1.5)
                    end
                    c:TweenSizeAndPosition(UDim2.new(0, size, 0, size), UDim2.new(0.5, (-size / 2), 0.5, (-size / 2)), 'Out', 'Quad', len, true, nil)
                    for i = 1, 10 do
                        c.ImageTransparency = c.ImageTransparency + 0.05
                        wait(len / 12)
                    end
                    c:Destroy()
                end)
            end

            DropdownFiller.Name = "DropdownFiller"
            DropdownFiller.Parent = DropdownFrameTop
            DropdownFiller.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            DropdownFiller.BorderSizePixel = 0
            DropdownFiller.Position = UDim2.new(0, 0, 1, -10)
            DropdownFiller.Size = UDim2.new(1, 0, 0, 10)
            DropdownFiller.ZIndex = 2

            Dropdown.Name = "Dropdown"
            Dropdown.Parent = Category
            Dropdown.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Dropdown.BackgroundTransparency = 1.000
            Dropdown.Size = UDim2.new(0, 100, 0, 100)
            Dropdown.ZIndex = 2
            Dropdown.Image = "rbxassetid://3570695787"
            Dropdown.ImageColor3 = Color3.fromRGB(30, 30, 30)
            Dropdown.ScaleType = Enum.ScaleType.Slice
            Dropdown.SliceCenter = Rect.new(100, 100, 100, 100)
            Dropdown.SliceScale = 0.040
            Dropdown.LayoutOrder = LayoutOrder
            LayoutOrder = LayoutOrder + 1

            DropdownName.Name = "DropdownName"
            DropdownName.Parent = Dropdown
            DropdownName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            DropdownName.BackgroundTransparency = 1.000
            DropdownName.Size = UDim2.new(0, 125, 1, 0)
            DropdownName.ZIndex = 2
            DropdownName.Font = Enum.Font.SourceSans
            DropdownName.Text = Text or "New Dropdown"
            DropdownName.TextColor3 = Color3.fromRGB(255, 255, 255)
            DropdownName.TextScaled = true
            DropdownName.TextSize = 14.000
            DropdownName.TextWrapped = true

            UIGridLayoutDropdown.Parent = DropdownButtons
            UIGridLayoutDropdown.SortOrder = Enum.SortOrder.LayoutOrder
            UIGridLayoutDropdown.CellPadding = UDim2.new(0, 0, 0, 0)
            UIGridLayoutDropdown.CellSize = UDim2.new(1, 0, 0, 25)

            local DropdownOpen = false

            DropdownButton.Name = "DropdownButton"
            DropdownButton.Parent = Dropdown
            DropdownButton.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            DropdownButton.BackgroundTransparency = 1.000
            DropdownButton.BorderSizePixel = 0
            DropdownButton.Position = UDim2.new(0, 125, 0, 0)
            DropdownButton.Size = UDim2.new(0, 215, 1, 0)
            DropdownButton.ZIndex = 3
            DropdownButton.Font = Enum.Font.SourceSans
            DropdownButton.Text = " Choose..."
            DropdownButton.TextColor3 = Color3.fromRGB(255, 255, 255)
            DropdownButton.TextScaled = true
            DropdownButton.TextSize = 14.000
            DropdownButton.TextWrapped = true
            DropdownButton.TextXAlignment = Enum.TextXAlignment.Left
            DropdownButton.MouseButton1Click:Connect(function()
                if DropdownOpen == false then
                    DropdownOpen = true
                    game:GetService("TweenService"):Create(Icon, TweenInfo.new(0.3), {Rotation = 45}):Play()
                    DropdownFrameTop.Visible = true
                    DropdownFrameTop:TweenSize(UDim2.new(0, 195, 0, 40), 'InOut', 'Linear', 0.15)
                    wait(0.15)
                    DropdownFrame:TweenSize(UDim2.new(0, 195, 0, 240), 'InOut', 'Linear', 0.15)
                else
                    DropdownOpen = false
                    game:GetService("TweenService"):Create(Icon, TweenInfo.new(0.3), {Rotation = 0}):Play()
                    DropdownFrame:TweenSize(UDim2.new(0, 195, 0, 0), 'InOut', 'Linear', 0.15)
                    wait(0.15)
                    DropdownFrameTop:TweenSize(UDim2.new(0, 0, 0, 40), 'InOut', 'Linear', 0.15)
                    wait(0.15)
                    DropdownFrameTop.Visible = false
                end
            end)

            DropdownRound.Name = "DropdownRound"
            DropdownRound.Parent = DropdownButton
            DropdownRound.Active = true
            DropdownRound.AnchorPoint = Vector2.new(0.5, 0.5)
            DropdownRound.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            DropdownRound.BackgroundTransparency = 1.000
            DropdownRound.Position = UDim2.new(0.5, 0, 0.5, 0)
            DropdownRound.Selectable = true
            DropdownRound.Size = UDim2.new(1, 0, 1, 0)
            DropdownRound.ZIndex = 2
            DropdownRound.Image = "rbxassetid://3570695787"
            DropdownRound.ImageColor3 = Color3.fromRGB(50, 50, 50)
            DropdownRound.ScaleType = Enum.ScaleType.Slice
            DropdownRound.SliceCenter = Rect.new(100, 100, 100, 100)
            DropdownRound.SliceScale = 0.040

            Icon.Name = "Icon"
            Icon.Parent = DropdownButton
            Icon.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Icon.BackgroundTransparency = 1.000
            Icon.Position = UDim2.new(1, -25, 0, 0)
            Icon.Size = UDim2.new(0, 25, 0, 25)
            Icon.ZIndex = 2
            Icon.Font = Enum.Font.SourceSans
            Icon.Text = "+"
            Icon.TextColor3 = Color3.fromRGB(255, 255, 255)
            Icon.TextSize = 30.000
            Icon.TextWrapped = true

            local Drop = {}

            function Drop:Choices(NewChoices)
                DropdownButtons:ClearAllChildren()
                DropdownButtons.CanvasSize = UDim2.new(0, 0, 0, 0)

                wait()

                local UIGridLayoutDropdown = Instance.new("UIGridLayout")

                UIGridLayoutDropdown.Parent = DropdownButtons
                UIGridLayoutDropdown.SortOrder = Enum.SortOrder.LayoutOrder
                UIGridLayoutDropdown.CellPadding = UDim2.new(0, 0, 0, 0)
                UIGridLayoutDropdown.CellSize = UDim2.new(1, 0, 0, 25)

                for i = 1, #NewChoices do
                    DropdownButtons.CanvasSize = DropdownButtons.CanvasSize + UDim2.new(0, 0, 0, 25)
                    local Choice = Instance.new("TextButton")
    
                    Choice.Name = "Choice"
                    Choice.Parent = DropdownButtons
                    Choice.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
                    Choice.BorderColor3 = Color3.fromRGB(30, 30, 30)
                    Choice.BorderSizePixel = 0
                    Choice.Size = UDim2.new(0, 200, 0, 50)
                    Choice.AutoButtonColor = false
                    Choice.Font = Enum.Font.SourceSans
                    Choice.Text = NewChoices[i]
                    Choice.TextColor3 = Color3.fromRGB(255, 255, 255)
                    Choice.TextScaled = true
                    Choice.TextSize = 14.000
                    Choice.TextWrapped = true
                    Choice.ClipsDescendants = true
    
                    Choice.MouseEnter:Connect(function()
                        game:GetService("TweenService"):Create(Choice, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(50,50,50)}):Play()
                    end)
            
                    Choice.MouseLeave:Connect(function()
                        game:GetService("TweenService"):Create(Choice, TweenInfo.new(0.3), {BackgroundColor3 = Color3.fromRGB(40,40,40)}):Play()
                    end)
    
                    Choice.MouseButton1Click:Connect(function()
                        DropdownButton.Text = " " .. Choice.Text
                        pcall(Callback, Choice.Text)
    
                        local c = Sample2:Clone()
                        c.Parent = Choice
                        local x, y = (ms.X - c.AbsolutePosition.X), (ms.Y - c.AbsolutePosition.Y)
                        c.Position = UDim2.new(0, x, 0, y)
                        local len, size = 1, nil
                        if Choice.AbsoluteSize.X >= Choice.AbsoluteSize.Y then
                            size = (Choice.AbsoluteSize.X * 1.5)
                        else
                            size = (Choice.AbsoluteSize.Y * 1.5)
                        end
                        c:TweenSizeAndPosition(UDim2.new(0, size, 0, size), UDim2.new(0.5, (-size / 2), 0.5, (-size / 2)), 'Out', 'Quad', len, true, nil)
                        for i = 1, 10 do
                            c.ImageTransparency = c.ImageTransparency + 0.05
                            wait(len / 12)
                        end
                        c:Destroy()
                    end)
                end
            end
            return Drop
        end
        return Page
    end
    return library
end

return LoadLib()
