            Label.Name = "Label"
            Label.Parent = Category
            Label.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
            Label.BackgroundTransparency = 1.000
            Label.BorderSizePixel = 0
            Label.Position = UDim2.new(0, 5, 0, 30)
            Label.Size = UDim2.new(0, 340, 0, 20)
            Label.ZIndex = 3
            Label.Font = Enum.Font.SourceSans
            Label.Text = Text or "New Label"
            Label.TextColor3 = Color3.fromRGB(255, 255, 255)
            Label.TextScaled = true
            Label.TextSize = 14.000
            Label.TextWrapped = true
            Label.LayoutOrder = LayoutOrder
            LayoutOrder = LayoutOrder + 1
            
            LabelRound.Name = "LabelRound"
            LabelRound.Parent = Label
            LabelRound.AnchorPoint = Vector2.new(0.5, 0.5)
            LabelRound.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            LabelRound.BackgroundTransparency = 1.000
            LabelRound.Position = UDim2.new(0.5, 0, 0.5, 0)
            LabelRound.Size = UDim2.new(1, 0, 1, 0)
            LabelRound.ZIndex = 2
            LabelRound.Image = "rbxassetid://3570695787"
